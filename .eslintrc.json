{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "airbnb",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "parser": "@babel/eslint-parser",
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "comma-dangle": [
            "error",
            "always"
        ],
    }
}
